{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "SeriLog.Sinks.File", "SeriLog.Sinks.Console" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "Path": "D:\\APILibraryLog\\Logs.txt",
          "rollingInterval": "Hour"
        }
      }
    ]
  }
}
